# Google Gemini API Key (for development/testing)
# The macOS app stores this in Keychain, but you can set it here for CLI tools
GEMINI_API_KEY=your_gemini_api_key_here

# Google Sheets Configuration (for development/testing)
# The macOS app configures this in Settings, but you can set it here for CLI tools
GOOGLE_SHEET_ID=your_google_sheet_id_here
GOOGLE_SERVICE_ACCOUNT_FILE=service_account.json
